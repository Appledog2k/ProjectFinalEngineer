<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="icon" type="image/x-icon" href="~/Images/logo/favicon.ico">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/siteIndex.css" />
    <link rel="stylesheet" href="~/tailwind/public/tailwind.css">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="false"></script>
</head>

<body>
    <div id="root" class="page page-dark">
        <div class="">
            <div>
                <main>
                    <div>
                        <div class="relative" style="background: linear-gradient(rgb(40, 64, 103), rgb(21, 31, 50));">
                            <div class="flex flex-col">
                                <div class="home-banner-inner flex flex-col">
                                    <nav style="position:fixed"
                                        class="section new-nav z-50 w-full max-w-none py-4 bg-transparent md:pt-7 md:pb-4"
                                        style="height: 60px;">
                                        <div class="relative flex h-full items-center justify-between">
                                            <div id="header-logo-arrow" class="mr-4 flex lg:mr-8 xl:mr-0 xl:w-1/4">
                                                <a aria-label="Laracasts"
                                                    class="relative inline-block cursor-pointer leading-none" href="/">
                                                    <img src="" width="145" alt="HUST">
                                                </a>
                                            </div>
                                            <div class="flex items-center justify-end">
                                                <button title="Tip: press / or s anywhere to instantly activate me."
                                                    class="leading-none rounded-xl border border-transparent bg-blue/10 p-3 leading-none text-white transition-all hover:border-blue/20 hover:text-blue">
                                                    <svg width="16" viewBox="0 0 15 15">
                                                        <g fill="none" fill-rule="evenodd">
                                                            <path d="M-2-2h20v20H-2z"></path>
                                                            <path class="fill-current"
                                                                d="M10.443 9.232h-.638l-.226-.218A5.223 5.223 0 0 0 10.846 5.6 5.247 5.247 0 1 0 5.6 10.846c1.3 0 2.494-.476 3.414-1.267l.218.226v.638l4.036 4.028 1.203-1.203-4.028-4.036zm-4.843 0A3.627 3.627 0 0 1 1.967 5.6 3.627 3.627 0 0 1 5.6 1.967 3.627 3.627 0 0 1 9.232 5.6 3.627 3.627 0 0 1 5.6 9.232z">
                                                            </path>

                                                        </g>
                                                    </svg>
                                                </button>
                                                <a href="/login"
                                                    class="link ml-6 flex-shrink-0 text-xs font-medium text-white hover:text-white">
                                                    Sign In </a>
                                            </div>
                                        </div>
                                    </nav>
                                    <div class="home-banner-main section flex w-full flex-1 flex-col overflow-hidden pt-10 pb-0 lg:mb-[-100px] lg:items-center"
                                        style="max-width: 100vw;">
                                        <div class="bc  mx-auto w-full flex-1">
                                            <div><img style="margin-top: -200px;" id="home-banner-illustration"
                                                    src="/images/home/home-root.svg"
                                                    alt="Lary the Laracasts robot and monster posing."
                                                    aria-label="Screencasts for the modern developer"
                                                    class="pointer-events-none  top-[-50px] left-[12vw] m-0 w-full scale-[1.5] mix-blend-luminosity lg:left-0 lg:top-0 lg:mt-[-231px] lg:ml-[-150px] lg:inline-block lg:w-auto lg:max-w-[1300px] lg:scale-100 xl:max-w-[1400px] widescreen:ml-0 widescreen:max-w-[1500px]">
                                            </div>
                                            <div
                                                class="code-menu hide-scrollbar top-0 z-10 mt-[50vw] h-auto w-full overflow-auto rounded-xl bg-blue-900 px-3 py-6 font-bold text-white lg:right-0 lg:ml-auto lg:mt-[80px] lg:w-[285px] xl:right-[5%] 2xl:right-[10%] widescreen:right-[18%]">
                                                <div class="font-bold">
                                                    <span class="font-bold text-[#ec454f]">"menu"</span>
                                                    " => [ "
                                                    <a class="code-link relative my-2 block rounded border-none pl-4 text-left text-blue hover:bg-blue hover:text-white"
                                                        href="#" style="margin-left: 10px; width: calc(100% - 10px);">
                                                        <div class="text-[.7rem] font-normal text-white/50">// deep
                                                            dives</div>
                                                        <span
                                                            class="font-bold text-white">"browse"&nbsp;=&gt;&nbsp;</span>
                                                        <span class="font-bold">"Series"</span>
                                                        <span class="text-white">,</span>
                                                    </a>
                                                    <a class="code-link relative my-2 block rounded border-none pl-4 text-left text-blue hover:bg-blue hover:text-white"
                                                        href="#" style="margin-left: 10px; width: calc(100% - 10px);">
                                                        <div class="text-[.7rem] font-normal text-white/50">// pick a
                                                            category</div>
                                                        <span class="font-bold text-white">"find" => </span>
                                                        <span class="font-bold">"Topics"</span>
                                                        <span class="text-white">,</span>
                                                    </a>
                                                    <a class="code-link relative my-2 block rounded border-none pl-4 text-left text-blue hover:bg-blue hover:text-white"
                                                        href="#" style="margin-left: 10px; width: calc(100% - 10px);">
                                                        <div class="text-[.7rem] font-normal text-white/50">// deep
                                                            dives</div>
                                                        <span
                                                            class="font-bold text-white">"browse"&nbsp;=&gt;&nbsp;</span>
                                                        <span class="font-bold">"Series"</span>
                                                        <span class="text-white">,</span>
                                                    </a>
                                                    <span class="font-bold">],</span>
                                                </div>
                                                <div class="mt-3 font-bold">
                                                    <span class="font-bold text-[#ec454f]">"stats"</span>
                                                    "=> [ "
                                                    <a class="code-link relative my-2 block rounded border-none pl-4 text-left text-blue hover:bg-blue hover:text-white"
                                                        href="/series"
                                                        style="margin-left: 10px; width: calc(100% - 10px);">
                                                        <div class="text-[.7rem] font-normal text-white/50">//
                                                            multi-episode training</div>
                                                        <span
                                                            class="font-bold text-white">"series"&nbsp;=&gt;&nbsp;</span>
                                                        <span class="font-bold">"176"</span>
                                                        <span class="text-white">,</span>
                                                    </a>
                                                    <a class="code-link relative my-2 block rounded border-none pl-4 text-left text-blue hover:bg-blue hover:text-white"
                                                        href="/series"
                                                        style="margin-left: 10px; width: calc(100% - 10px);">
                                                        <div class="text-[.7rem] font-normal text-white/50">//
                                                            multi-episode training</div>
                                                        <span
                                                            class="font-bold text-white">"series"&nbsp;=&gt;&nbsp;</span>
                                                        <span class="font-bold">"176"</span>
                                                        <span class="text-white">,</span>
                                                    </a>
                                                    <span class="font-bold">]</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </main>
            </div>
        </div>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
</body>

</html>
